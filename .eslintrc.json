{
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": 1,
    "no-console": 0,
    "no-unused-vars": 1,
    "new-cap": [2, {
      "capIsNewExceptions": [
        "Router"
      ]
    }]
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true
  }
}